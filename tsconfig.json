{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"strictNullChecks": true,
		"target": "ES2022",
		"moduleResolution": "Node16",
		"module": "Node16",
		"declaration": true,
		"isolatedModules": true,
		"noEmit": true,
		"outDir": "dist"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
